var data = {
	"show": "Doctor Who",
	"characters": [
	{"name": "The Doctor", "role": "Protagonist"},
	{"name": "Amy", "role": "Companion"},
	{"name": "Rory", "role": "Companion 2"},
	{"name": "Dalek", "role": "Antagonist"}
	]
};

for(var prop in data){
	//console.log(data[prop]);
	for(var x in data[prop]){
		//EXAMPLE CONSOLE
		//console.log(data[prop][x]);
		for(var y in data[prop][x]){
			console.log(data[prop][x][y]);
		}
	}
}

